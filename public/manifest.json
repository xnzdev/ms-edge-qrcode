{
  "name": "Best QRcode(快速生成二维码)",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "最好的二维码生成器，轻松转化二维码. A Chromium Extension to show the QRcode of the link.",
  "icons": {
    "16": "icons/qrcode-16x16.png",
    "32": "icons/qrcode-32x32.png",
    "48": "icons/qrcode-48x48.png",
    "128": "icons/qrcode-128x128.png"
  },
  "offline_enabled": true,
  "browser_action": {
    "default_popup": "/index.html"
  },
  "permissions": [
    "tabs"
  ],
  "background":{
    "scripts": ["/umi.9f347862.js"],
    "persistent": false
  }
}